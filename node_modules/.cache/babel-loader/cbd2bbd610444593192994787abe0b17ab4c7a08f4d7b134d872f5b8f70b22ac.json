{"ast":null,"code":"import axios from 'axios'; // import axios\n\nexport default {\n  name: 'FileImport',\n  data() {\n    return {\n      progress: 0,\n      file: null\n    };\n  },\n  methods: {\n    handleDrop(event) {\n      event.preventDefault();\n      const files = event.dataTransfer.files;\n      // 处理拖放的文件\n      this.handleFile(files);\n    },\n    handleFileInput(event) {\n      const files = event.target.files;\n      // 处理所选文件\n      this.handleFile(files);\n    },\n    handleClick() {\n      this.$refs.fileInput.click();\n      console.log('点击事件已触发'); // added to check if the click event is triggered\n    },\n\n    handleFile(files) {\n      if (files[0]) {\n        // check if files[0] exists\n        const allowedTypes = ['nii', 'nii.gz', 'gz'];\n        const fileType = files[0].name.split('.').pop();\n        if (allowedTypes.includes(fileType)) {\n          // check if the file type is allowed\n          this.file = files[0];\n          console.log('Selected file:', this.file.name);\n          this.uploadFile();\n        } else {\n          alert('文件类型不允许，请上传 nii、nii.gz 或 gz 格式的文件');\n        }\n      } else {\n        console.log(\"出错了\");\n      }\n    },\n    uploadFile() {\n      if (this.file) {\n        const formData = new FormData();\n        formData.append('name', this.file.name);\n        formData.append('file', this.file);\n        formData.append('type', 'original');\n        console.log('FormData object:', formData.get('name'));\n\n        //通过axios发送ajax请求到后端数据库，保存文件\n        axios.post('http://localhost:3001/save-data', formData, {\n          onUploadProgress: progressEvent => {\n            this.progress = Math.round(progressEvent.loaded / progressEvent.total * 100);\n          }\n        }).then(res => {\n          console.log(res.data);\n          alert('文件已成功上传');\n        }).catch(err => {\n          console.error(err);\n          alert('上传失败，请重试');\n        });\n      } else {\n        alert('请先选择要上传的文件');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","progress","file","methods","handleDrop","event","preventDefault","files","dataTransfer","handleFile","handleFileInput","target","handleClick","$refs","fileInput","click","console","log","allowedTypes","fileType","split","pop","includes","uploadFile","alert","formData","FormData","append","get","post","onUploadProgress","progressEvent","Math","round","loaded","total","then","res","catch","err","error"],"sources":["E:\\HomeWork\\百度飞浆\\medical_0.1.0\\src\\components\\Import\\FileImport.vue"],"sourcesContent":["<template>\r\n  <div class=\"file-upload\">\r\n    <div class=\"drop-zone\" @dragover.prevent @drop=\"handleDrop\" @click=\"handleClick\">\r\n      <p>将.nii.gz格式文件拖放到此处或单击以选择文件</p>\r\n      <input type=\"file\" ref=\"fileInput\" @change=\"handleFileInput\" accept=\".nii.gz\" style=\"display: block; width: 0; height: 0; opacity: 0;\"/>\r\n    </div>\r\n\r\n    <div class=\"progress-bar\">\r\n      <div class=\"progress\" :style=\"{ width: progress + '%' }\"></div>\r\n    </div>\r\n\r\n    <button class=\"StartPredict\" @click=\"StartPredict\">开始预测</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'; // import axios\r\n\r\nexport default {\r\n  name:'FileImport',\r\n  data() {\r\n    return {\r\n      progress: 0,\r\n      file: null\r\n    }\r\n  },\r\n  methods: {\r\n    handleDrop(event) {\r\n      event.preventDefault();\r\n      const files = event.dataTransfer.files;\r\n      // 处理拖放的文件\r\n      this.handleFile(files);\r\n    },\r\n    handleFileInput(event) {\r\n      const files = event.target.files;\r\n      // 处理所选文件\r\n      this.handleFile(files);\r\n    },\r\n    handleClick() {\r\n      this.$refs.fileInput.click();\r\n      console.log('点击事件已触发'); // added to check if the click event is triggered\r\n    },\r\n    handleFile(files) {\r\n      if (files[0]) { // check if files[0] exists\r\n        const allowedTypes = ['nii', 'nii.gz', 'gz'];\r\n        const fileType = files[0].name.split('.').pop();\r\n        if (allowedTypes.includes(fileType)) { // check if the file type is allowed\r\n          this.file = files[0];\r\n          console.log('Selected file:', this.file.name);\r\n          this.uploadFile();\r\n        } else {\r\n          alert('文件类型不允许，请上传 nii、nii.gz 或 gz 格式的文件');\r\n        }\r\n      } else {\r\n        console.log(\"出错了\")\r\n      }\r\n    },\r\n    uploadFile() {\r\n      if (this.file) {\r\n        const formData = new FormData();\r\n        formData.append('name', this.file.name);\r\n        formData.append('file', this.file);\r\n        formData.append('type', 'original');\r\n        console.log('FormData object:', formData.get('name'));\r\n\r\n        //通过axios发送ajax请求到后端数据库，保存文件\r\n        axios.post('http://localhost:3001/save-data', formData, {\r\n          onUploadProgress: progressEvent => {\r\n            this.progress = Math.round((progressEvent.loaded / progressEvent.total) * 100);\r\n          }\r\n        })\r\n          .then(res => {\r\n            console.log(res.data);\r\n            alert('文件已成功上传');\r\n          })\r\n          .catch(err => {\r\n            console.error(err);\r\n            alert('上传失败，请重试');\r\n          });\r\n      } else {\r\n        alert('请先选择要上传的文件');\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.file-upload {\r\n  display: flex;\r\n  margin-top: 80px;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.drop-zone {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  width: 60%;\r\n  height: 200px;\r\n  border: 2px dashed #ccc;\r\n  border-radius: 50px; /* 圆角更圆 */\r\n  margin-bottom: 1rem;\r\n  cursor: pointer;\r\n}\r\n\r\n.drop-zone p {\r\n  margin: 0;\r\n}\r\n\r\nbutton {\r\n  margin-top: 1rem;\r\n  padding: 0.5rem 1rem;\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 0.25rem;\r\n  cursor: pointer;\r\n}\r\n\r\n.progress-bar {\r\n  width: 60%;\r\n  height: 20px;\r\n  background-color: #f5f5f5;\r\n  border-radius: 10px;\r\n  overflow: hidden;\r\n  margin-top: 1rem;\r\n}\r\n\r\n.progress {\r\n  height: 100%;\r\n  background-color: #007bff;\r\n  border-radius: 10px;\r\n  transition: width 0.3s ease-in-out;\r\n}\r\n</style>"],"mappings":"AAgBA,OAAOA,KAAI,MAAO,OAAO,EAAE;;AAE3B,eAAe;EACbC,IAAI,EAAC,YAAY;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CAAC;MACXC,IAAI,EAAE;IACR;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAACC,KAAK,EAAE;MAChBA,KAAK,CAACC,cAAc,EAAE;MACtB,MAAMC,KAAI,GAAIF,KAAK,CAACG,YAAY,CAACD,KAAK;MACtC;MACA,IAAI,CAACE,UAAU,CAACF,KAAK,CAAC;IACxB,CAAC;IACDG,eAAeA,CAACL,KAAK,EAAE;MACrB,MAAME,KAAI,GAAIF,KAAK,CAACM,MAAM,CAACJ,KAAK;MAChC;MACA,IAAI,CAACE,UAAU,CAACF,KAAK,CAAC;IACxB,CAAC;IACDK,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACC,KAAK,CAACC,SAAS,CAACC,KAAK,EAAE;MAC5BC,OAAO,CAACC,GAAG,CAAC,SAAS,CAAC,EAAE;IAC1B,CAAC;;IACDR,UAAUA,CAACF,KAAK,EAAE;MAChB,IAAIA,KAAK,CAAC,CAAC,CAAC,EAAE;QAAE;QACd,MAAMW,YAAW,GAAI,CAAC,KAAK,EAAE,QAAQ,EAAE,IAAI,CAAC;QAC5C,MAAMC,QAAO,GAAIZ,KAAK,CAAC,CAAC,CAAC,CAACR,IAAI,CAACqB,KAAK,CAAC,GAAG,CAAC,CAACC,GAAG,EAAE;QAC/C,IAAIH,YAAY,CAACI,QAAQ,CAACH,QAAQ,CAAC,EAAE;UAAE;UACrC,IAAI,CAACjB,IAAG,GAAIK,KAAK,CAAC,CAAC,CAAC;UACpBS,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAE,IAAI,CAACf,IAAI,CAACH,IAAI,CAAC;UAC7C,IAAI,CAACwB,UAAU,EAAE;QACnB,OAAO;UACLC,KAAK,CAAC,mCAAmC,CAAC;QAC5C;MACF,OAAO;QACLR,OAAO,CAACC,GAAG,CAAC,KAAK;MACnB;IACF,CAAC;IACDM,UAAUA,CAAA,EAAG;MACX,IAAI,IAAI,CAACrB,IAAI,EAAE;QACb,MAAMuB,QAAO,GAAI,IAAIC,QAAQ,EAAE;QAC/BD,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACzB,IAAI,CAACH,IAAI,CAAC;QACvC0B,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACzB,IAAI,CAAC;QAClCuB,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,UAAU,CAAC;QACnCX,OAAO,CAACC,GAAG,CAAC,kBAAkB,EAAEQ,QAAQ,CAACG,GAAG,CAAC,MAAM,CAAC,CAAC;;QAErD;QACA9B,KAAK,CAAC+B,IAAI,CAAC,iCAAiC,EAAEJ,QAAQ,EAAE;UACtDK,gBAAgB,EAAEC,aAAY,IAAK;YACjC,IAAI,CAAC9B,QAAO,GAAI+B,IAAI,CAACC,KAAK,CAAEF,aAAa,CAACG,MAAK,GAAIH,aAAa,CAACI,KAAK,GAAI,GAAG,CAAC;UAChF;QACF,CAAC,EA