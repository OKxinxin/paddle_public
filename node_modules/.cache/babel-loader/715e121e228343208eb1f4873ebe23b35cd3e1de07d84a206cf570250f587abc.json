{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios'; // import axios\n\nexport default {\n  name: 'FileImport',\n  methods: {\n    handleDrop(event) {\n      event.preventDefault();\n      const files = event.dataTransfer.files;\n      // 处理拖放的文件\n      this.handleFile(files);\n    },\n    handleFileInput(event) {\n      const files = event.target.files;\n      // 处理所选文件\n      this.handleFile(files);\n    },\n    handleClick() {\n      this.$refs.fileInput.click();\n      console.log('点击事件已触发'); // added to check if the click event is triggered\n    },\n\n    handleFile(files) {\n      const formData = new FormData();\n      const allowedTypes = ['nii', 'nii.gz', 'gz'];\n      if (files[0]) {\n        // check if files[0] exists\n        const fileType = files[0].name.split('.').pop();\n        if (allowedTypes.includes(fileType)) {\n          // check if the file type is allowed\n          formData.append('name', files[0].name);\n          formData.append('file', files[0]);\n          formData.append('type', 'original');\n          console.log('FormData object:', formData.get('name'));\n\n          //通过axios发送ajax请求到后端数据库，保存文件\n          axios.post('http://localhost:3001/save-data', formData).then(res => {\n            console.log(res.data);\n            alert('文件已成功上传');\n            this.$router.push('/display');\n          }).catch(err => {\n            console.error(err);\n            console.log(\"shibai\");\n            alert('上传失败，请重试');\n          });\n        } else {\n          alert('文件类型不允许，请上传 nii、nii.gz 或 gz 格式的文件');\n        }\n      } else {\n        console.log(\"出错了\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","methods","handleDrop","event","preventDefault","files","dataTransfer","handleFile","handleFileInput","target","handleClick","$refs","fileInput","click","console","log","formData","FormData","allowedTypes","fileType","split","pop","includes","append","get","post","then","res","data","alert","$router","push","catch","err","error"],"sources":["E:\\HomeWork\\百度飞浆\\medical_0.1.0\\src\\components\\Import\\FileImport.vue"],"sourcesContent":["<template>\r\n  <div class=\"file-upload\">\r\n    <div class=\"drop-zone\" @dragover.prevent @drop=\"handleDrop\" @click=\"handleClick\">\r\n      <p>将.nii.gz格式文件拖放到此处或单击以选择文件</p>\r\n      <input type=\"file\" ref=\"fileInput\" @change=\"handleFileInput\" accept=\".nii.gz\" style=\"display: block; width: 0; height: 0; opacity: 0;\"/>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'; // import axios\r\n\r\nexport default {\r\n  name:'FileImport',\r\n  methods: {\r\n    handleDrop(event) {\r\n      event.preventDefault();\r\n      const files = event.dataTransfer.files;\r\n      // 处理拖放的文件\r\n      this.handleFile(files);\r\n    },\r\n    handleFileInput(event) {\r\n      const files = event.target.files;\r\n      // 处理所选文件\r\n      this.handleFile(files);\r\n    },\r\n    handleClick() {\r\n      this.$refs.fileInput.click();\r\n      console.log('点击事件已触发'); // added to check if the click event is triggered\r\n    },\r\n    handleFile(files) {\r\n      const formData = new FormData();\r\n      const allowedTypes = ['nii', 'nii.gz', 'gz'];\r\n    if (files[0]) { // check if files[0] exists\r\n      const fileType = files[0].name.split('.').pop();\r\n      if (allowedTypes.includes(fileType)) { // check if the file type is allowed\r\n        formData.append('name', files[0].name);\r\n        formData.append('file', files[0]);\r\n        formData.append('type', 'original');\r\n        console.log('FormData object:', formData.get('name'));\r\n\r\n      //通过axios发送ajax请求到后端数据库，保存文件\r\n      axios.post('http://localhost:3001/save-data', formData)\r\n        .then(res => {\r\n          console.log(res.data);\r\n          alert('文件已成功上传');\r\n          this.$router.push('/display');\r\n        })\r\n        .catch(err => {\r\n          console.error(err);\r\n          console.log(\"shibai\");\r\n          alert('上传失败，请重试');\r\n        });\r\n      } else {\r\n        alert('文件类型不允许，请上传 nii、nii.gz 或 gz 格式的文件');\r\n      }\r\n    } else {\r\n      console.log(\"出错了\")\r\n    }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.file-upload {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.drop-zone {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  width: 60%;\r\n  height: 200px;\r\n  border: 2px dashed #ccc;\r\n  border-radius: 5px;\r\n  margin-bottom: 1rem;\r\n  cursor: pointer;\r\n}\r\n\r\n.drop-zone p {\r\n  margin: 0;\r\n}\r\n</style>\r\n\r\n"],"mappings":";AAUA,OAAOA,KAAI,MAAO,OAAO,EAAE;;AAE3B,eAAe;EACbC,IAAI,EAAC,YAAY;EACjBC,OAAO,EAAE;IACPC,UAAUA,CAACC,KAAK,EAAE;MAChBA,KAAK,CAACC,cAAc,EAAE;MACtB,MAAMC,KAAI,GAAIF,KAAK,CAACG,YAAY,CAACD,KAAK;MACtC;MACA,IAAI,CAACE,UAAU,CAACF,KAAK,CAAC;IACxB,CAAC;IACDG,eAAeA,CAACL,KAAK,EAAE;MACrB,MAAME,KAAI,GAAIF,KAAK,CAACM,MAAM,CAACJ,KAAK;MAChC;MACA,IAAI,CAACE,UAAU,CAACF,KAAK,CAAC;IACxB,CAAC;IACDK,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACC,KAAK,CAACC,SAAS,CAACC,KAAK,EAAE;MAC5BC,OAAO,CAACC,GAAG,CAAC,SAAS,CAAC,EAAE;IAC1B,CAAC;;IACDR,UAAUA,CAACF,KAAK,EAAE;MAChB,MAAMW,QAAO,GAAI,IAAIC,QAAQ,EAAE;MAC/B,MAAMC,YAAW,GAAI,CAAC,KAAK,EAAE,QAAQ,EAAE,IAAI,CAAC;MAC9C,IAAIb,KAAK,CAAC,CAAC,CAAC,EAAE;QAAE;QACd,MAAMc,QAAO,GAAId,KAAK,CAAC,CAAC,CAAC,CAACL,IAAI,CAACoB,KAAK,CAAC,GAAG,CAAC,CAACC,GAAG,EAAE;QAC/C,IAAIH,YAAY,CAACI,QAAQ,CAACH,QAAQ,CAAC,EAAE;UAAE;UACrCH,QAAQ,CAACO,MAAM,CAAC,MAAM,EAAElB,KAAK,CAAC,CAAC,CAAC,CAACL,IAAI,CAAC;UACtCgB,QAAQ,CAACO,MAAM,CAAC,MAAM,EAAElB,KAAK,CAAC,CAAC,CAAC,CAAC;UACjCW,QAAQ,CAACO,MAAM,CAAC,MAAM,EAAE,UAAU,CAAC;UACnCT,OAAO,CAACC,GAAG,CAAC,kBAAkB,EAAEC,QAAQ,CAACQ,GAAG,CAAC,MAAM,CAAC,CAAC;;UAEvD;UACAzB,KAAK,CAAC0B,IAAI,CAAC,iCAAiC,EAAET,QAAQ,EACnDU,IAAI,CAACC,GAAE,IAAK;YACXb,OAAO,CAACC,GAAG,CAACY,GAAG,CAACC,IAAI,CAAC;YACrBC,KAAK,CAAC,SAAS,CAAC;YAChB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,UAAU,CAAC;UAC/B,