orScroll = false;\r\n      this.$router.push('/about');\r\n    }\r\n  },\r\n},\r\nmounted() {\r\n  if (this.$route.path === '/') {\r\n    window.addEventListener('scroll', this.scrollToBottom);\r\n  }\r\n},\r\nbeforeUnmount() {\r\n  window.removeEventListener('scroll', this.scrollToBottom);\r\n},\r\ndata() {\r\n  return {\r\n    shouldWaitForScroll: false, // 记录用户是否已经滚动到底部\r\n  };\r\n},\r\n}\r\n\r\n\r\n</script>\r\n\r\n<style scoped>\r\n/* 在这里编写组件的样式 */\r\n\r\n\r\n</style>"],"mappings":";;;EAESA,KAAK,EAAC,MAAM;EAACC,KAA0E,EAA1E;IAAA;IAAA;IAAA;IAAA;EAAA;;gEAElBC,mBAAA,CAAa,YAAT,MAAI;;;uBAFRC,mBAAA,CAGI,OAHJC,UAGI,GAFJC,mBAAA,gBAAmB,EACnBC,UAAa,C"},"metadata":{},"sourceType":"module","externalDependencies":[]}