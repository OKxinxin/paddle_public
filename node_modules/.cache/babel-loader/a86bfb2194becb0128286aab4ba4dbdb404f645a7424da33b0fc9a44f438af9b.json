 };\n  },\n  watch: {\n    \"$route\": function(to, from) {\n      this.setTransitionName(to, from);\n    },\n  },\n  methods: {\n    setTransitionName(to, from) {\n      let transitionName = \"\";\n      if (from.name === \"Page1\" && to.name === \"Page2\") {\n        transitionName = \"slide-up\";\n      } else if (from.name === \"Page1\" && to.name === \"Page3\") {\n        transitionName = \"slide-left\";\n      } else if (from.name === \"Page2\" && to.name === \"Page1\") {\n        transitionName = \"slide-down\";\n      } else if (from.name === \"Page2\" && to.name === \"Page3\") {\n        transitionName = \"slide-left\";\n      } else if (from.name === \"Page3\" && to.name === \"Page1\") {\n        transitionName = \"slide-right\";\n      } else if (from.name === \"Page3\" && to.name === \"Page2\") {\n        transitionName = \"slide-right\";\n      }\n      this.transitionName = transitionName;\n    },\n  },\n};\n</script>\n\n<style>\n\n.slide-up-enter-active,\n.slide-down-leave-active,\n.slide-left-enter-active,\n.slide-right-leave-active {\n  transition: all 0.5s ease-in-out;\n}\n\n.slide-up-enter,\n.slide-down-leave-to,\n.slide-left-enter,\n.slide-right-leave-to {\n  transform: translate3d(0, 100%, 0);\n}\n\n.slide-down-enter,\n.slide-up-leave-to,\n.slide-right-enter,\n.slide-left-leave-to {\n  transform: translate3d(0, -100%, 0);\n}\n\n.slide-left-enter,\n.slide-right-leave-to {\n  transform: translate3d(100%, 0, 0);\n}\n\n.slide-right-enter,\n.slide-left-leave-to {\n  transform: translate3d(-100%, 0, 0);\n}\n\n/* .fade-enter-active,\n.fade-leave-active {\n  transition: opacity 0.5s;\n}\n\n.fade-enter,\n.fade-leave-to {\n  opacity: 0;\n}\n\n.slide-enter-active,\n.slide-leave-active {\n  transition: transform 0.5s;\n}\n\n.slide-enter,\n.slide-leave-to {\n  transform: translateX(100%);\n} */\n\n\n</style>\n"],"mappings":"AAmBA,OAAOA,MAAK,MAAO,qCAAqC;AAExD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVF;EACJ,CAAC;EAEDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACLC,cAAc,